extends layout

block content
    div.card-columns
        each res in results
            div.card
                if res.coordinates
                    img(src= res.coordinates.ja360_image_url).card-img-top.img-fluid
                div.card-block
                    h4.card-title= res.name
                    p.card-text= address_nominative
                    a(href="#").btn.btn-secondary Velja

